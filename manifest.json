{
  "name": "Reload All Tabs",
  "version": "0.1.0",
  "manifest_version": 2,
  "description": "A chrome extension to reload all tabs at once.",
  "permissions": [
    "tabs"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_title": "Reload All Tabs"
  }
}
